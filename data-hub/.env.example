# Restaurant Data Hub Environment Variables

# Supabase (별도 인스턴스 - 한식당과 분리)
DATA_HUB_DATABASE_URL=postgresql://postgres:password@db.xxxxx.supabase.co:6543/postgres

# Google Gemini AI (한식당과 공유 가능)
GEMINI_API_KEY=your_gemini_api_key_here

# Scraping APIs
APIFY_API_TOKEN=your_apify_token_here
OUTSCRAPER_API_KEY=your_outscraper_key_here
BRIGHT_DATA_PROXY_URL=http://username:password@proxy.brightdata.com:22225

# 한식당 연동
HANSIKDANG_API_URL=http://localhost:5000
DATA_COLLECTION_API_KEY=your_data_collection_api_key_here

# Optional Settings
DAILY_TARGET=333
QUALITY_THRESHOLD=70
BATCH_SIZE=500
